{
  "name": "spesr",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "cd ../pb && ./spesr serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "generate:sdk": "swagger2openapi ../pb/docs/swagger.yaml -o ../pb/docs/openapi.yaml && openapi-typescript ../pb/docs/openapi.yaml --output ./src/lib/pocketbase/generated-sdk.ts",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@sveltejs/adapter-auto": "^2.1.1",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/enhanced-img": "^0.2.0",
    "@sveltejs/kit": "^1.30.4",
    "@types/node": "^20.12.11",
    "@vincjo/datatables": "^1.14.10",
    "@vite-pwa/sveltekit": "^0.5.0",
    "autoprefixer": "^10.4.19",
    "openapi-typescript": "^7.0.0",
    "pocketbase-typegen": "^1.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "sass": "^1.77.1",
    "svelte": "^4.2.16",
    "svelte-check": "^3.7.1",
    "svelte-headless-table": "^0.8.3",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "vite": "^4.5.3"
  },
  "type": "module",
  "dependencies": {
    "@internationalized/date": "^3.5.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/js-yaml": "^4.0.9",
    "ajv": "^8.13.0",
    "axios": "^1.7.2",
    "bits-ui": "^0.21.9",
    "clsx": "^2.1.1",
    "cmdk-sv": "^0.0.17",
    "gridjs": "^6.2.0",
    "gridjs-svelte": "^2.1.1",
    "js-yaml": "^4.1.0",
    "lucide-svelte": "^0.379.0",
    "mode-watcher": "^0.3.0",
    "openapi-fetch": "^0.10.1",
    "pocketbase": "^0.21.3",
    "svelte-french-toast": "^1.2.0",
    "svelte-legos": "^0.2.3",
    "svelte-radix": "^1.1.0",
    "svelte-sonner": "^0.3.24",
    "tailwind-merge": "^2.3.0",
    "tailwind-variants": "^0.2.1",
    "tesseract.js": "^5.1.0",
    "vaul-svelte": "^0.3.1"
  }
}
